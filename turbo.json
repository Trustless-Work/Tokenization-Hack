{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".next/**",
        "out/**",
        "!.next/cache/**"
      ]
    },
    "start": {
      "cache": false,
      "dependsOn": ["build"]
    },
    "test": {
      "dependsOn": ["^test"]
    }
  }
}
